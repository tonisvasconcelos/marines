# Frontend Environment Variables
# Copy this file to .env.local for local development

# API Base URL
# For local development: leave empty to use relative paths (/api)
# For production: set to your backend URL (e.g., https://api.yourdomain.com)
VITE_API_URL=

# App Base URL (optional, for absolute URLs if needed)
# VITE_APP_BASE_URL=http://localhost:3000

# Public API Keys (if any)
# VITE_MAP_API_KEY=your_map_api_key_here
